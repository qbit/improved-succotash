@extraunexec rm -rf ${LOCALSTATEDIR}/db/filebeat/{*,registry/filebeat/*}
@extraunexec rm -rf ${LOCALSTATEDIR}/log/filebeat/*
@rcscript ${RCDIR}/filebeat
@bin bin/filebeat
