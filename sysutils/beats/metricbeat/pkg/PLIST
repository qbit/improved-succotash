@extraunexec rm -rf ${LOCALSTATEDIR}/db/metricbeat/*
@extraunexec rm -rf ${LOCALSTATEDIR}/log/metricbeat/*
@rcscript ${RCDIR}/metricbeat
@bin bin/metricbeat
