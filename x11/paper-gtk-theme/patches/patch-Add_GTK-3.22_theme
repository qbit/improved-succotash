+      box-shadow:inset 0 -1px 0 0 $borders_color;
+
+      &:active {
+        box-shadow:inset 0 -3px 0 0 $borders_color;
+
+        &:backdrop {
+          box-shadow:inset 0 -3px 0 0 $backdrop_borders_color;
